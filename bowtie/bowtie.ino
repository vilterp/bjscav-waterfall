void setup() {
        Serial.begin(9600);
	//initializes all pins as digital
	//range is determined by constans above
	for(int i = sol_to_pin(0); i < sol_to_pin(35) ; i++)
		pinMode(i, OUTPUT);
        Serial.println("starting");
}

void set_pins_for_line(int row) {
	for(int col = 0; col < 35; col++) {
		digitalWrite(sol_to_pin(col), image_data[row][col] ? HIGH : LOW); 
	}
}

int lines = 32;

int currentDepth = lines - 1;

void loop() {
	set_pins_for_line(currentDepth);
	delay(10);
        Serial.println(currentDepth);
	currentDepth--;
	if(currentDepth == 0) {
                delay(500);
		currentDepth = lines - 1;
	}
}

int sol_to_pin(int n) {
	return n + 2;
}


